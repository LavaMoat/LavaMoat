{
  "name": "lavamoat-viz",
  "version": "7.0.3",
  "description": "This is a dashboard for exploring a dependency graph and LavaMoat policy file",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LavaMoat/lavamoat.git",
    "directory": "packages/viz"
  },
  "homepage": "https://github.com/LavaMoat/LavaMoat/blob/main/packages/viz/README.md",
  "bugs": {
    "url": "https://github.com/LavaMoat/lavamoat/issues"
  },
  "author": "kumavis <aaron@kumavis.me>",
  "license": "MIT",
  "engines": {
    "node": "^16.20.0 || ^18.0.0 || ^20.0.0"
  },
  "bin": {
    "lavamoat-viz": "bin/index.js"
  },
  "main": "index.js",
  "files": [
    ".browserslistrc",
    "bin",
    "dist"
  ],
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build": "webpack --config ./src/webpack.config.js",
    "clean": "rimraf ./dist",
    "deploy": "npm run rebuild && gh-pages -d dist",
    "rebuild": "npm run clean && npm run build",
    "test": "npm run build"
  },
  "dependencies": {
    "lavamoat-core": "^15.1.2",
    "ncp": "2.0.0",
    "open": "8.4.2",
    "pify": "5.0.0",
    "serve-handler": "6.1.5",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@babel/core": "7.23.9",
    "@babel/preset-env": "7.23.9",
    "@babel/preset-react": "7.23.3",
    "arr-union": "3.1.0",
    "babel-loader": "8.3.0",
    "clone-deep": "4.0.1",
    "codemirror": "5.65.16",
    "copy-webpack-plugin": "6.4.1",
    "css-loader": "5.2.7",
    "d3": "7.8.5",
    "esm": "3.2.25",
    "gh-pages": "6.1.1",
    "html-webpack-plugin": "4.5.2",
    "kind-of": "6.0.3",
    "react": "16.14.0",
    "react-bootstrap": "2.10.0",
    "react-codemirror2": "7.3.0",
    "react-dom": "16.14.0",
    "react-force-graph-2d": "1.25.3",
    "react-hyperscript": "3.2.0",
    "rimraf": "5.0.5",
    "statsvr": "3.1.4",
    "style-loader": "2.0.0",
    "three": "0.160.1",
    "three-forcegraph": "1.41.10",
    "three-spritetext": "1.8.2",
    "webpack": "4.47.0",
    "webpack-cli": "4.10.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      [
        "@babel/preset-env",
        {
          "debug": true
        }
      ]
    ]
  },
  "servedPath": "./"
}
