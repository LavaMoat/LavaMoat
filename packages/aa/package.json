{
  "name": "@lavamoat/aa",
  "version": "4.3.2",
  "description": "LavaMoat's secure package naming convention",
  "author": "kumavis",
  "license": "MIT",
  "engines": {
    "node": "^16.20.0 || ^18.0.0 || ^20.0.0 || ^22.0.0"
  },
  "bin": {
    "lavamoat-ls": "src/cli.js"
  },
  "main": "src/index.js",
  "types": "./types/index.d.ts",
  "directories": {
    "test": "test"
  },
  "files": [
    "CHANGELOG.md",
    "src",
    "types",
    "!*.tsbuildinfo",
    "!tsconfig.json"
  ],
  "scripts": {
    "bench": "node ./test/benchmark/index.js",
    "lint:deps": "depcheck",
    "test": "npm run test:run",
    "test:run": "ava"
  },
  "dependencies": {
    "resolve": "1.22.10"
  },
  "devDependencies": {
    "@types/resolve": "1.20.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "ava": {
    "files": [
      "test/*.spec.js"
    ],
    "timeout": "30s"
  }
}
