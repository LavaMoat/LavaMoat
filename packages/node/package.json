{
  "name": "@lavamoat/node",
  "version": "0.1.0",
  "type": "module",
  "description": "Next-gen LavaMoat for Node.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/LavaMoat/LavaMoat.git",
    "directory": "packages/node"
  },
  "homepage": "https://github.com/LavaMoat/LavaMoat/tree/main/packages/node",
  "author": "LavaMoat Project",
  "license": "MIT",
  "engines": {
    "node": "^18.0.0 || ^20.0.0 <20.16.1 || ^22.5.1",
    "npm": ">=9.0.0"
  },
  "bin": {
    "lavamoat2": "./src/cli.js"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./src/index.js"
    },
    "./attenuator": {
      "types": "./types/default-attenuator.d.ts",
      "import": "./src/default-attenuator.js"
    },
    "./package.json": "./package.json"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint:deps": "depcheck",
    "test": "npm run test:run",
    "test:run": "ava",
    "update-policy": "node ./scripts/update-own-policy.js && git add -A ./src/policy-override.json"
  },
  "dependencies": {
    "@endo/compartment-mapper": "^1.2.2",
    "@endo/evasive-transform": "1.3.0",
    "@types/node": "18.19.28",
    "json-stable-stringify": "1.1.1",
    "lavamoat-core": "^16.0.0",
    "ses": "1.8.0",
    "type-fest": "4.26.1",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@endo/eslint-plugin": "2.1.0",
    "@jessie.js/eslint-plugin": "0.4.0",
    "@types/json-stable-stringify": "1.0.36",
    "memfs": "4.8.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "ava": {
    "files": [
      "test/**/*.spec.js"
    ],
    "timeout": "10s"
  }
}
